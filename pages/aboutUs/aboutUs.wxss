.title{
  position: relative;
  padding-bottom: 50rpx;
}

.title image{
  width: 750rpx;
  height: 301rpx;
  position: absolute;
  z-index: -1;
}

.title view{
  padding-top: 250rpx;
  font-size: 60rpx;
  line-height: 42rpx;
  color:lightblue;
  font-weight: bold;
}

.body{
  border:10rpx dotted #ffcc00;background:#fffff7;
  border-radius: 40rpx;
  margin: 40rpx;
  margin-bottom: 80rpx;
}

.firstPart{
  margin: 30rpx;
  text-align: center;
  border-bottom: 3px dashed rgba(0, 0, 0, 0.1);
  font-size: 35rpx;
  line-height: 49rpx;
}

.secondPart{
  margin: 30rpx;
  font-size: 35rpx;
  line-height: 49rpx;
}

.texttitle{
  font-size: 40rpx;
  display:block;
  width:300rpx;
  height:40rpx;
  position:relative;
  top: -30rpx;
  left: 25rpx;
  text-align: center;
  background: white;
}